{
    "app": "colabfold",
    "verbose": true,
    "server" : {
        "address"    : "127.0.0.1:8888",
        "dbmanagment": false,
        "cors"       : true
    },
    "paths" : {
        "databases"    : "/mnt/sdc/colabfold_db",
        "results"      : "~/jobs",
        "temporary"    : "~/tmp",
        "colabfold"    : {
            "parallelstages": true,
            "uniref"        : "/mnt/sdc/colabfold_db/uniref30_2202_db",
            "pdb"           : "/mnt/sdc/colabfold_db/pdb70_220313",
            "environmental" : "/mnt/sdc/colabfold_db/colabfold_envdb_202108_db",
            "pdb70"         : "/mnt/sdc/colabfold_db/pdb70",
            "pdbdivided"    : "/mnt/sdc/colabfold_db/pdb/divided",
            "pdbobsolete"   : "/mnt/sdc/colabfold_db/pdb/obsolete"
        },
        "mmseqs"       : "~/mmseqs/bin/mmseqs"
    },
    "local" : {
        "workers"  : 16
    },
    "redis" : {
        "network"  : "tcp",
        "address"  : "mmseqs-web-redis:6379",
        "password" : "",
        "index"    : 0
    },
    "mail" : {
        "type"      : "null",
        "sender"    : "mail@example.org",
        "templates" : {
            "success" : {
                "subject" : "Done -- %s",
                "body"    : "Dear User,\nThe results of your submitted job are available now at https://search.mmseqs.com/queue/%s .\n"
            },
            "timeout" : {
                "subject" : "Timeout -- %s",
                "body"    : "Dear User,\nYour submitted job timed out. More details are available at https://search.mmseqs.com/queue/%s .\nPlease adjust the job and submit it again.\n"
            },
            "error"   : {
                "subject" : "Error -- %s",
                "body"    : "Dear User,\nYour submitted job failed. More details are available at https://search.mmseqs.com/queue/%s .\nPlease submit your job later again.\n"
            }
        }
    }
}
